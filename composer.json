{
  "name": "bytic/config",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/config",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Config\\": "src"
    },
    "files": [
      "src/functions/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Config\\Tests\\": "tests/src",
      "Nip\\Config\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.1|^8.0",
    "symfony/config": "^4.0|^5.0|^6.0"
  },
  "require-dev": {
    "bytic/container": "^0.9|^1.0|^2.0",
    "bytic/console": "^0.9|^1.0",
    "bytic/phpqatools": "^1.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
